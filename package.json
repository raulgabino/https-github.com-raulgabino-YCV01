{
  "name": "your-city-vibe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "release": "turbo run build --filter=!@your-city-vibe/web && changeset publish",
    "smoke-test": "tsx scripts/smoke.ts",
    "test:smoke": "npm run smoke-test",
    "type-check": "turbo run type-check",
    "version-packages": "changeset version"
  },
  "dependencies": {
    "@upstash/redis": "latest",
    "@your-city-vibe/api": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "openai": "latest",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "latest",
    "swr": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ws": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8.5",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "turbo": "^1.11.0",
    "typescript": "^5"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=18.17.0"
  }
}